{
  "name": "aero-team-eindhoven",
  "version": "0.0.0",
  "private": true,
  "description": "Aero Team Eindhoven Website",
  "author": "aeroteameindhoven",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "typecheck": "tsc",
    "lint": "eslint src/**/*.{ts,tsx}",
    "__fmt": "prettier src types gatsby-config.ts .eslintrc .yarnrc.yml package.json tsconfig.json .prettierrc .github .vscode",
    "fmt:check": "yarn __fmt --check",
    "fmt:write": "yarn __fmt --write",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "decap": "decap-server",
    "serve": "gatsby serve"
  },
